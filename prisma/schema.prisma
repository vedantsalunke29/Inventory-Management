generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id               Int     @id @default(autoincrement())
  name             String
  description      String?
  stock_quantity   Int     @default(0)
  low_stock_threshold Int? 
  createdAt        DateTime @default(now())
  updatedAt        DateTime @updatedAt
}
